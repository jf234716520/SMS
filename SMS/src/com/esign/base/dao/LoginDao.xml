<?xml version="1.0" encoding="UTF-8" ?>    
<!DOCTYPE mapper    
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"    
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.esign.base.dao.LoginDao">  
	
	<select id="confirmAcc" parameterType="string" resultType="map">
		select * from sys_user_account where user_id=#{userId} and passwd=#{passwd}
	</select>
	
</mapper> 